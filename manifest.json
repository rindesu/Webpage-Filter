{  
    "name": "BBSFilter",  
    "version": "1.0",  
	"manifest_version":2,
    "description": "BBSFilter",  
    "browser_action": {  
        "default_icon": "icon.png"
		
    },
    "icons": {  
        "128": "icon_128.png"  
    },  
	"background": {     "scripts": ["background.js"]},
    "permissions": [  
        "tabs", 
		"http://*/",
		"https://*/",
		"notifications",  
		"contextMenus"	
    ] 
}  